<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.banking">

    <application
        android:icon="@mipmap/banking_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/banking_icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HistoryActivity"></activity>
        <activity
            android:name=".SplashScreen"
            android:theme="@style/SplashScreenTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CustsomerList" />
        <activity
            android:name=".CustomerList"
            android:label="@string/title_activity_customer_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity"></activity>
    </application>

</manifest>